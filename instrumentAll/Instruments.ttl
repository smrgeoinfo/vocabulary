inst:TOOL0430
  rdf:type skos:Concept ;
  dcterm:date "2011-11-02 11:37:25.0" ;
  dcterm:identifier "SDN:L22::TOOL0430" ;
  pav:hasCurrentVersion <http://vocab.nerc.ac.uk/collection/L22/current/TOOL0430/1> ;
  owl:versionInfo "1" ;
  skos:altLabel "Secchi disk"@en ;
  skos:broader <http://vocab.nerc.ac.uk/collection/L05/current/124> ;
  skos:definition "A circular disk, typically 20cm in diameter, used to measure water transparency. For marine applications, the disk is usually entirely white, while for limnological studies it is common to use one with alternating black and white quadrants. The reasons for this may be historical rather thah theoretical. A black Secchi disk has recently been developed for use in shallow rivers and streams. The disk is lowered slowly into the water on a pole or line until it can no longer be seen by the observer. The depth of disappearance provides an estimate of the transparency of the water, although any reading is affected by the observer's eyesight and ambient light conditions."@en ;
  skos:inScheme <http://vocab.nerc.ac.uk/scheme/SDNDEV/current> ;
  skos:notation "SDN:L22::TOOL0430" ;
  skos:note "accepted"@en ;
  skos:prefLabel "Secchi disk"@en ;
  skos:related <http://vocab.nerc.ac.uk/collection/B75/current/ORG00001> ;
.
