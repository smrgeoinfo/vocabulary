  skos:prefLabel "Furuno Navigator GP170 Global Positioning System receiver"@en ;
  skos:related <http://vocab.nerc.ac.uk/collection/B75/current/ORG00571> ;
  skos:related <http://vocab.nerc.ac.uk/collection/L35/current/MAN0115> ;
